#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main ()
{
	int numero,contador=0;
	cout<<"Ingrese un numero natural:  ";
	cin>>numero;
	do
	{numero=numero/10;
	contador++;
	}
	while (numero>0);
	cout<<"el numero elegido tiene digitos:  "<<contador;
	getch();

}
